<rpc semp-version="soltr/%s">
  <message-spool>
    <vpn-name>%s</vpn-name>
    <event>
      <spool-usage>
        <thresholds>
          <set-percentage>%s</set-percentage>
          <clear-percentage>%s</clear-percentage>
        </thresholds>
      </spool-usage>
    </event>
  </message-spool>
</rpc>
