 <rpc semp-version="soltr/%s">
<bridge>
    <bridge-name>%s</bridge-name>
    <vpn-name>%s</vpn-name>
    <primary/>
    <remote>
      <message-vpn>
        <vpn-name>%s</vpn-name>
        <connect-via/>
        <addr>%s</addr>
        <connect-order>
          <number>%s</number>
        </connect-order>
      </message-vpn>
    </remote>
  </bridge>
</rpc>
