<rpc semp-version="soltr/%s">
  <bridge>
    <bridge-name>%s</bridge-name>
    <vpn-name>%s</vpn-name>
    <primary/>
    <remote>
      <message-vpn>
        <vpn-name>%s</vpn-name>
        <connect-via/>
        <addr>%s</addr>
        <message-spool>
          <window-size>
            <number>%s</number>
          </window-size>
        </message-spool>
      </message-vpn>
    </remote>
  </bridge>
</rpc>
